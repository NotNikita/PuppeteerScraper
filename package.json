{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "A basic typescript app starter for 2023.",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "tsc",
    "start": "node dist/index.js",
    "start:dev": "npx --env-file=.env nodemon",
