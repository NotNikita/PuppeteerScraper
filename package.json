    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "telegraf": "^4.15.0",
    "user-agents": "^1.1.50"
  },
  "pkg": {
    "assets": "node_modules/puppeteer-extra-plugin-stealth/**/*.*"
